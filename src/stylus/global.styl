@import '../assets/fonts/font.styl'


.slide-right-enter-active,
.slide-right-leave-active,
.slide-left-enter-active,
.slide-left-leave-active {
  transition: all .2s;
}
.slide-right-enter {
  transform: translateX(-100%);
}
.slide-left-enter {
  transform: translateX(100%);
}

/**
 * 动画 浅入浅出
 */
.fadesd-enter-active,
.fadesd-leave-active
  transition: all .2s ease

.fadesd-enter,
.fadesd-leave-active
  opacity: 0

